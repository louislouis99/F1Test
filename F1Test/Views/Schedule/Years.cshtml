@model IEnumerable<int>

@{
    ViewBag.Title = "Years";
}

<div class="row">
    @foreach (var year in Model)
    {
        <div class="col-auto">
            @Html.ActionLink(year.ToString(), "Races", new { id = year }, htmlAttributes: new { @class = "btn btn-warning" })
        </div>
    }
</div>

<div>
    @Html.ActionLink("Back", "Index")
</div>
