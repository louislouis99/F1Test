@model IEnumerable<int>

@{
    ViewBag.Title = "Years";
}

    <div style="margin-top:60px;">
        <a href="javascript:history.back()" class="m-1">
            <i class="fas fa-chevron-left text-danger"></i>
        </a>
        <span class="font-weight-bold">Years</span>

        <div class="row mt-3">
            @foreach (var year in Model)
            {
                <div class="col-auto p-3">
                    @Html.ActionLink(year.ToString(), "Races", new { id = year },
                   htmlAttributes: new { @class = "btn btn-warning" })
                </div>
            }
        </div>
    </div>