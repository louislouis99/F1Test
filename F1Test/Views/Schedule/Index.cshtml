@model IEnumerable<int>

@{
    ViewBag.Title = "Index";
}

<div style="margin-top:60px;">
    <span class="font-weight-bold">Decades</span>
    <div class="row mt-3">
        @foreach (var decade in Model)
        {
            <div class="col-auto p-2">
                @Html.ActionLink(decade.ToString(), "Years", new { id = decade },
                         htmlAttributes: new { @class = "btn btn-primary" })
            </div>
        }
    </div>
</div>

