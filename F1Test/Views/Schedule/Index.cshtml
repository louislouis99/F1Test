@model IEnumerable<int>

@{
    ViewBag.Title = "Index";
}

<div class="row">
    @foreach (var decade in Model)
    {
        <div class="col-auto">
            @Html.ActionLink(decade.ToString() ,"Years", new { id = decade }, htmlAttributes: new { @class = "btn btn-primary" })
        </div>
    }
</div>

